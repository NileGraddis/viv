{
  "name": "@hms-dbmi/viv",
  "version": "0.5.0",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist",
    "src"
  ],
  "description": "High bit depth, high resolution, many channels, low overhead",
  "scripts": {
    "build": "rollup -c",
    "build:w": "rollup -c --watch",
    "test": "npm run test:layers && npm run test:loaders",
    "test:layers": "TEST_LAYERS=true rollup -c | browserify - | tape-run | tap-spec",
    "test:loaders": "TEST_LOADERS=true rollup -c | node - | tap-spec",
    "version": "./version.sh",
    "postversion": "git push && git push --tags",
    "docs": "mkdir -p dist_docs || rm -r dist_docs && mkdir -p dist_docs && documentation build src/** -f html --config documentation.yml --shallow -o dist_docs"
  },
  "pre-push": [
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hms-dbmi/viv.git"
  },
  "keywords": [],
